{
  "background": {
    "service_worker": "js/background.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "js": ["js/myscript.js"],
      "matches": ["<all_urls>"],
      "run_at": "document_end"
    }
  ],
  "description": "Reload images in Tab/Window using context menu.",
  "developer": {
    "name": "mubaidr",
    "url": "https://github.com/mubaidr"
  },
  "homepage_url": "https://github.com/mubaidr/reloadImage",
  "icons": {
    "128": "img/128.png"
  },
  "manifest_version": 3,
  "name": "Reload Images",
  "permissions": ["contextMenus"],
  "version": "3.0.0"
}
